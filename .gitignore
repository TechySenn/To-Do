# Dependency directories
#------------------------------------------------------------------------------
# Typically node_modules should not be committed to Git.
# Netlify will install dependencies based on package.json during build.
/node_modules

# Logs
#------------------------------------------------------------------------------
# Log files are typically specific to runs and environment, not code.
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
lerna-debug.log*
.pnpm-debug.log*

# Diagnostic reports (https://nodejs.org/api/report.html)
#------------------------------------------------------------------------------
report.[0-9]*.[0-9]*.[0-9]*.[0-9]*.json

# Runtime data
#------------------------------------------------------------------------------
# Files related to specific runtime states or processes.
pids
*.pid
*.seed
*.pid.lock

# Directory for instrumented libs generated by jscoverage/JSCover
#------------------------------------------------------------------------------
lib-cov

# Coverage directory used by tools like istanbul
#------------------------------------------------------------------------------
coverage
*.lcov

# nyc test coverage
#------------------------------------------------------------------------------
.nyc_output

# Grunt intermediate storage (https://gruntjs.com/creating-plugins#storing-task-files)
#------------------------------------------------------------------------------
.grunt

# node-waf configuration
#------------------------------------------------------------------------------
.lock-wscript

# Compiled binary addons (https://nodejs.org/api/addons.html)
#------------------------------------------------------------------------------
build/Release

# Dependency directories
#------------------------------------------------------------------------------
# These are often included in node_modules, but good to list explicitly.
jspm_packages/
web_modules/

# Optional eslint cache
#------------------------------------------------------------------------------
.eslintcache

# Optional stylelint cache
#------------------------------------------------------------------------------
.stylelintcache

# Microbundle cache
#------------------------------------------------------------------------------
.rpt2_cache/
.rts2_cache_cjs/
.rts2_cache_es/
.rts2_cache_umd/

# Optional REPL history
#------------------------------------------------------------------------------
.node_repl_history

# Output of 'npm pack'
#------------------------------------------------------------------------------
*.tgz

# Yarn Integrity file
#------------------------------------------------------------------------------
.yarn-integrity

# dotenv environment variables file
#------------------------------------------------------------------------------
# Never commit your environment variables.
.env
.env.development.local
.env.test.local
.env.production.local
.env.local

# parcel-bundler cache (https://parceljs.org/)
#------------------------------------------------------------------------------
.cache
.parcel-cache

# Next.js build output
#------------------------------------------------------------------------------
.next
out

# Nuxt.js build output
#------------------------------------------------------------------------------
.nuxt
dist

# Gatsby files
#------------------------------------------------------------------------------
.cache/
# Add node_modules folders within the Gatsby cache directory to the ignore list
.cache/**/node_modules
public

# Mac specific
#------------------------------------------------------------------------------
.DS_Store
Thumbs.db

# Windows specific
#------------------------------------------------------------------------------
desktop.ini

# Optional IDE/Editor directories
#------------------------------------------------------------------------------
.vscode/
.idea/

